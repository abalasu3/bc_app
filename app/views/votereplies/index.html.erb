<h1>Listing votereplies</h1>

<table>
  <tr>
    <th>Rcount</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @votereplies.each do |votereply| %>
  <tr>
    <td><%= votereply.rcount %></td>
    <td><%= vote_post.reply_id %></td>
    <td><%= vote_post.user_id %></td>
    <td><%= link_to 'Show', votereply %></td>
    <td><%= link_to 'Edit', edit_votereply_path(votereply) %></td>
    <td><%= link_to 'Destroy', votereply, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Votereply', new_votereply_path %>
